  <!-- Added manually -->
  <build>
    <plugins>
      <plugin>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-maven-plugin</artifactId>
      </plugin>

      <!-- If we want to generate at compile time the schema augmentation for debug developer purpose -->
      <!-- If not wanted comment this line and the dependency -->
      <plugin>
        <groupId>org.neo4j</groupId>
        <artifactId>neo4j-graphql-augmented-schema-generator-maven-plugin</artifactId>
        <version>${neo4j-graphql-augmented-schema-generator-maven-plugin.version}</version>
        <executions>
          <execution>
            <goals>
              <goal>generate-schema</goal>
            </goals>
            <configuration>
              <schemaConfig>
                <pluralizeFields>true</pluralizeFields>
                <useWhereFilter>false</useWhereFilter>
                <queryOptionStyle>INPUT_TYPE</queryOptionStyle>
                <mutation>
                  <enabled>true</enabled>
                </mutation>
              </schemaConfig>
              <outputDirectory>${project.build.directory}/augmented-schema</outputDirectory>
              <fileset>
                <directory>${project.basedir}/src/main/resources/graphql</directory>
                <include>*.graphql</include>
              </fileset>
            </configuration>
          </execution>
        </executions>
      </plugin>

    </plugins>
  </build>
